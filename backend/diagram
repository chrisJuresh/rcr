---
Django ER Diagram
---
erDiagram
LogEntry {
    AutoField id
    DateTimeField action_time
    ForeignKey user
    ForeignKey content_type
    TextField object_id
    CharField object_repr
    PositiveSmallIntegerField action_flag
    TextField change_message
}
Permission {
    AutoField id
    CharField name
    ForeignKey content_type
    CharField codename
}
Group {
    AutoField id
    CharField name
    ManyToManyField permissions
}
ContentType {
    AutoField id
    CharField app_label
    CharField model
}
Session {
    CharField session_key
    TextField session_data
    DateTimeField expire_date
}
User {
    BigAutoField id
    CharField password
    DateTimeField last_login
    BooleanField is_superuser
    CharField first_name
    CharField last_name
    BooleanField is_staff
    BooleanField is_active
    DateTimeField date_joined
    CharField email
    CharField title
    ForeignKey trust
    BooleanField trust_approved
    ManyToManyField groups
    ManyToManyField user_permissions
    ManyToManyField roles
}
UserRole {
    BigAutoField id
    ForeignKey user
    ForeignKey role
    BooleanField requested
    BooleanField approved
}
ReviewerInfo {
    OneToOneField user_role
    ManyToManyField specialities
}
RepresentativeInfo {
    OneToOneField user_role
    ManyToManyField specialities
}
TrustEmployeeInfo {
    OneToOneField user_role
}
RCREmployeeInfo {
    OneToOneField user_role
}
UnauthenticatedUser {
    BigAutoField id
    CharField email
    CharField password
    CharField token
}
Region {
    BigAutoField id
    CharField name
}
Trust {
    BigAutoField id
    CharField name
    ForeignKey region
}
Role {
    BigAutoField id
    CharField name
}
ConsultantType {
    BigAutoField id
    CharField name
}
Speciality {
    BigAutoField id
    CharField name
    ForeignKey consultant_type
}
JD {
    BigAutoField id
    FileField file
    DateTimeField submission_date
    ForeignKey trust
    ForeignKey consultant_type
    ManyToManyField primary_specialities
    ManyToManyField sub_specialities
}
AAC {
    BigAutoField id
    DateTimeField submission_date
    DateField date
    ManyToManyField jd
}
LogEntry }|--|| User : user
LogEntry }|--|| ContentType : content_type
Permission }|--|| ContentType : content_type
Group }|--|{ Permission : permissions
User }|--|| Trust : trust
User }|--|{ Group : groups
User }|--|{ Permission : user_permissions
User }|--|{ Role : roles
UserRole }|--|| User : user
UserRole }|--|| Role : role
ReviewerInfo ||--|| UserRole : user_role
ReviewerInfo }|--|{ Speciality : specialities
RepresentativeInfo ||--|| UserRole : user_role
RepresentativeInfo }|--|{ Speciality : specialities
TrustEmployeeInfo ||--|| UserRole : user_role
RCREmployeeInfo ||--|| UserRole : user_role
Trust }|--|| Region : region
Speciality }|--|| ConsultantType : consultant_type
JD }|--|| Trust : trust
JD }|--|| ConsultantType : consultant_type
JD }|--|{ Speciality : primary_specialities
JD }|--|{ Speciality : sub_specialities
AAC }|--|{ JD : jd